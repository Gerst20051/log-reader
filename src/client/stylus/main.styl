html
  font-size 1em

body
  background #2e4557
  color #fff
  font-family inconsolata, "Courier New", monospace
  font-size 16px
  line-height 24px
  -webkit-tap-highlight-color rgba(0, 0, 0, 0)
  -webkit-tap-highlight-color transparent
  -webkit-touch-callout none
  -webkit-user-select none
  -khtml-user-select none
  -moz-user-select none
  -ms-user-select none
  user-select none
  word-break break-all

#logs
  margin-left 130px
  max-width 1600px
  padding 0 1rem

#logs .log-row
  cursor pointer
  margin-bottom 0.75rem
  padding 12px
  position relative

#logs .log-row:nth-child(even)
  background-color #304f5f
  border-bottom 1px solid #365a6f
  border-left 1px solid #24292c
  border-radius 8px
  border-right 1px solid #365a6f
  border-top 1px solid #24292c

#logs .log-row-data
  -webkit-touch-callout default
  -webkit-user-select text
  -khtml-user-select text
  -moz-user-select text
  -ms-user-select text
  user-select text

#logs .log-row-details
  background-color #414740
  display none
  margin-top 5px

.nowrap
  white-space nowrap important
